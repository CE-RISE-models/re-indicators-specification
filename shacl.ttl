@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix re_ind: <https://ce-rise-models.codeberg.page/re-indicators-specification/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

re_ind:Assessment a sh:NodeShape ;
    sh:closed true ;
    sh:description "An actual assessment of a product" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Version of the model used for this assessment" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path re_ind:model_version ],
        [ sh:datatype xsd:string ;
            sh:description "Reference to IndicatorSpecification used (e.g., REcycle_PV)" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path re_ind:indicator_specification_id ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path re_ind:id ],
        [ sh:class re_ind:ParameterAssessment ;
            sh:description "Assessment results for each parameter" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path re_ind:parameter_assessments ],
        [ sh:datatype xsd:dateTime ;
            sh:description "When assessment was performed" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path re_ind:timestamp ],
        [ sh:class re_ind:ProductInfo ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path re_ind:product_info ],
        [ sh:datatype xsd:float ;
            sh:description "Overall computed score" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path re_ind:total_score ] ;
    sh:targetClass re_ind:Assessment .

re_ind:IndicatorProductConfiguration a sh:NodeShape ;
    sh:closed true ;
    sh:description "Configuration linking an indicator to a product with specific parameters" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Product category this applies to" ;
            sh:in ( "PV" "Battery" "Laptop" "Printer" "HVAC" "Heatpump" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path re_ind:product_category ],
        [ sh:class re_ind:ParameterApplication ;
            sh:description "Parameters with their weights for this product" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path re_ind:parameter_applications ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path re_ind:description ],
        [ sh:description "Type of indicator (REcycle, REuse, etc.)" ;
            sh:in ( "REuse" "REpair" "REmanufacture" "REfurbish" "REcycle" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path re_ind:indicator_type ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path re_ind:name ],
        [ sh:datatype xsd:string ;
            sh:description "Unique ID like \"REcycle_PV\"" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path re_ind:id ] ;
    sh:targetClass re_ind:IndicatorProductConfiguration .

re_ind:ParameterSpecification a sh:NodeShape ;
    sh:closed false ;
    sh:description "A parameter within an indicator with its questions" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path re_ind:id ],
        [ sh:datatype xsd:string ;
            sh:description "e.g., \"Documentation availability\"" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path re_ind:name ],
        [ sh:class re_ind:QuestionSpecification ;
            sh:nodeKind sh:IRI ;
            sh:order 5 ;
            sh:path re_ind:questions ],
        [ sh:datatype xsd:boolean ;
            sh:description "Whether this is a bonus parameter" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path re_ind:is_bonus ],
        [ sh:datatype xsd:float ;
            sh:description "Weight in the overall indicator score" ;
            sh:maxCount 1 ;
            sh:maxInclusive 1e+00 ;
            sh:minCount 1 ;
            sh:minInclusive 0e+00 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path re_ind:weight ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path re_ind:description ] ;
    sh:targetClass re_ind:ParameterSpecification .

